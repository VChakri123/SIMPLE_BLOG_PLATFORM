<mvc:View controllerName="com.app.blogplatform.controller.View3"
		height="100%"
		xmlns:mvc="sap.ui.core.mvc"
		 xmlns:html="http://www.w3.org/1999/xhtml"
		xmlns="sap.m"
		xmlns:tnt="sap.tnt">
	
     <tnt:ToolPage id="toolPage1">
	    <tnt:header>
            <tnt:ToolHeader
                id="_IDGenToolHeader333"
                class="header"
            >
                <Button
                    id="sideNavigationToggleButton333"
                    icon="sap-icon://menu2"
                    type="Transparent"
                    press=".onSideNavButtonPress"
                >
                    <layoutData>
                        <OverflowToolbarLayoutData
                            id="_IDGenOverflowToolbarLayoutData333"
                            priority="NeverOverflow"
                        />
                    </layoutData>
                </Button>

                <Image
                    id="_IDGenImage333"
                    height="1.5rem"
                    class="sapUiSmallMarginBegin"
                    src="https://www.sap.com/dam/application/shared/logos/sap-logo-svg.svg"
                />

                <Title
                    level="H1"
                    text="SIMPLE-BLOG-PLATFORM"
                    wrapping="false"
                    class="TitleMain"
                    id="blogPlatform333"
                >
                    <layoutData>
                        <OverflowToolbarLayoutData
                            id="_IDGenOverflowToolbarLayoutData1333"
                            priority="Disappear"
                        />
                    </layoutData>
                </Title>
                <Text
                    wrapping="false"
                    id="secondTitle333"
                    class="TitleBOX"
                >
                    <layoutData>
                        <OverflowToolbarLayoutData
                            id="_IDGenOverflowToolbarLayoutData2333"
                            priority="Disappear"
                        />
                    </layoutData>
                </Text>

                <ToolbarSpacer
                    id="_IDGenToolbarSpacer333"
                    width="1.125rem"
                />

                <ToolbarSpacer id="_IDGenToolbarSpacer1333">
                    <layoutData>
                        <OverflowToolbarLayoutData
                            id="_IDGenOverflowToolbarLayoutData3333"
                            priority="NeverOverflow"
                            minWidth="20px"
                        />
                    </layoutData>
                </ToolbarSpacer>

                <Button
                    id="idTextName2333"
                    text="Back"
                    type="Accept"
                    press=".handleUserNamePress2"
                    ariaHasPopup="Menu"
                >
                    <layoutData>
                        <OverflowToolbarLayoutData
                            id="_IDGenOverflowToolbarLayoutData6333"
                            priority="NeverOverflow"
                        />
                    </layoutData>
                </Button>

                <Button
                    id="idTextName1333"
                    text="view3"
                    type="Up"
                    press="handleUserNamePresss1"
                    ariaHasPopup="Menu"
                >
                    <layoutData>
                        <OverflowToolbarLayoutData
                            id="_IDGenOverflowToolbarLayoutData5333"
                            priority="NeverOverflow"
                        />
                    </layoutData>
                </Button>
                
                <Button
                    id="idTextName333"
                    text="Chakradhar Reddy"
                    type="Transparent"
                    press=".handleUserNamePress3"
                    ariaHasPopup="Menu"
                >
                    <layoutData>
                        <OverflowToolbarLayoutData
                            id="_IDGenOverflowToolbarLayoutData7333"
                            priority="NeverOverflow"
                        />
                    </layoutData>
                </Button>
                <Avatar
                    id="_IDGenAvatar333"
                    src="test-resources/sap/tnt/images/person_avatar_01.png"
                    displaySize="XS"
                    press="onAvatarPressed"
                    tooltip="Profile"
                >
                    <layoutData>
                        <OverflowToolbarLayoutData
                            id="_IDGenOverflowToolbarLayoutData8333"
                            priority="NeverOverflow"
                        />
                    </layoutData>
                </Avatar>
            </tnt:ToolHeader>
        </tnt:header>
		  </tnt:ToolPage>
</mvc:View>
